.title {
  margin-top: 100px;
  margin-left: 30px;
  margin-right: 30px;
  text-align: center;
  font-style: italic;
  font-size: larger;
  font-weight: bold;
}

.img {
  width: 100px;
  margin-top: 20px;
  margin-left: 100px;
  margin-right: 100px;
}

.item {
    font-size: 24px;
    text-align: left;
}

.input {
    width: 250px;
    height: 50px;
    border: 1px solid ligthgray;
    border-radius: 10px;
}

.confirm {
  font-weight: normal;
  color: white;
  background-color: rgb(106, 170, 9);
  margin-top: 50px;
  margin-left: 100px;
  margin-right: 100px;
  box-shadow: 5px 5px 10px grey;
}

.reset {
  font-weight: normal;
  background-color: rgb(136, 132, 132);
  margin-top: 20px;
  margin-left: 100px;
  margin-right: 100px;
  box-shadow: 5px 5px 10px grey;
}

.view-contain-ti {
  text-align: left;
  display: flex;
  height: 40px;
  margin: 20px;
  border: 3rpx solid #1f1609;
  border-radius: 10rpx;
}

.text-ti {
  text-align: left;
  position: absolute;
  font-size: 12px;
  background: white;
  margin: -10px 0 0 10px;
  padding: 0 5px;
  color: rgb(144, 147, 167);
}